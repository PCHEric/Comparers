{
  "version": "1.0.0-eta-01",
  "authors": [ "Stephen Cleary" ],
  "description": "The last comparison library you'll ever need!",
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "packOptions": {
    "tags": [ "comparer", "equalitycomparer", "icomparable", "iequatable" ],
    "projectUrl": "https://github.com/StephenCleary/Comparers",
    "licenseUrl": "https://raw.githubusercontent.com/StephenCleary/Comparers/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "https://github.com/StephenCleary/Comparers.git"
    }
  },
  "frameworks": {
    "netstandard1.0": {
      "dependencies": {
        "System.Collections": "4.0.11",
        "System.Reflection": "4.1.0"
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile259": {
      "frameworkAssemblies": {
        "System.Collections": { "type": "build" },
        "System.Reflection": { "type": "build" },
        "System.Runtime": { "type": "build" }
      }
    }
  }
}
